      <description>$hostdir=&amp;quot;./&amp;quot;; //获取环境目录 $fiel_array = scandir($hostdir); //获取文件夹内的文件及文件夹名存入数组 $fiel_array //var_dump($fiel_array); foreach ($$fiel_array as $key=&amp;gt; $value) { if($value === &#39;.&#39; || $value === &#39;..&#39;){ continue; } $url=&amp;quot;文件名称：&amp;quot;.$value; $aurl= &amp;quot;&amp;quot;.$url.&amp;quot;&amp;quot;; echo $aurl . &amp;quot; &amp;quot;; } </description>
